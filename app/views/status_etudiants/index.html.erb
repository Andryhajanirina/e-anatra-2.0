     <section class="col-md-offset-3 padding-top-3x padding-left-right">
        <h1 class="mobile-center">Voici votre statut <span class="text-semibold"><%= current_user.first_name %> <%= current_user.name %></span></h1>
        <div class="row padding-top">
           <div class="col-sm-8 padding-bottom-2x">
              
              <!-- Tab Panes -->
              <div class="tab-content">
                 <div role="tabpanel" class="tab-pane transition fade scale in active" id="profile">
                    <form method="post">
                       
                       <div class="row">
                          
                          
            <table>

                <% if @statut != nil
                    @statut.each do |f| %>
                        <tr><td>Province</td><td>:</td><td><%= f.province.province_name %></td></tr>
                        <tr><td>Etablissement</td><td>:</td><td><%= f.etab.nom_etab %></td></tr>
                        <tr><td>Filiere</td><td>:</td><td><%= f.filiere.filiere %></td></tr>
                        <tr><td>Niveau</td><td>:</td><td><%= f.niveau.niveau %></td></tr>
                        <tr><td>Debut Session</td><td>:</td><td><%= Date.parse(f.vague.debut_session.to_s).strftime('%a %d %b %Y') %></td></tr>
                        <tr><td>Fin Session</td><td>:</td><td><%= Date.parse(f.vague.fin_session.to_s).strftime('%a %d %b %Y') %></td></tr>
                        <tr>
                            <td>Status</td><td>:</td>
                            <% if f.valide == true %>
                                <td style="color: #65c11f;">Inscription validé </td>
                            <% else %>
                                <td style="color: #ff0000;">Validation d'inscription en attente</td>
                            <% end %>
                        </tr>
                        <tr><td>+++++++++++++++++++++</td><td>+++++++++</td><td>+++++++++++++++++++++</td></tr>
                    <% end %>
                <% else %>
                    <tr><td>Vous n'êtes pas inscrit ou votre inscription n'est pas validé !!!</td></tr>
                    <tr><td><a href="<%= new_inscription_path %>" >Fêtes votre inscription!!</a></td></tr>                    
                <% end %>
                        
            </table>

                       </div>
                       
                        
                    </form>
                 </div>
              </div>
              <!-- .tab-content -->
           </div>
           <!-- .col-sm-8 -->
        </div>
        <!-- .row -->
     </section>